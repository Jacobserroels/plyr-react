{
  "name": "plyr-react",
  "version": "4.0.0",
  "description": "A simple HTML5, YouTube and Vimeo player for react using plyr",
  "keywords": [
    "react",
    "plyr",
    "video",
    "vimeo",
    "youtube",
    "accessibility",
    "javascript",
    "player",
    "media"
  ],
  "homepage": "https://chintan9.github.io/plyr-react/",
  "repository": {
    "type": "git",
    "url": "git@github.com:chintan9/plyr-react.git"
  },
  "license": "MIT",
  "author": "Chintan Prajapati",
  "main": "dist/src/index.js",
  "module": "dist/src/index.es.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development BABEL_ENV=development run-p build:watch start",
    "start": "cross-env NODE_ENV=development BABEL_ENV=development ",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production rollup -c && npm run copy && tsc --emitDeclarationOnly",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --forceExit --colors",
    "lint": "esw --ext .tsx --ext .ts --color",
    "lint:fix": "npm run lint --fix",
    "prepare": "npm run build && husky install",
    "prerelease": "npm run build",
    "release": "npm publish",
    "predeploy": "npm run prepare",
    "deploy": "gh-pages -d ",
    "copy": "npm run copy:css",
    "copy:css": "cpy node_modules/plyr/dist/plyr.css dist/ --flat"
  },
  "dependencies": {
    "plyr": "^3.7.2",
    "react-aptor": "^2.0.0-alpha.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@rollup/plugin-url": "^7.0.0",
    "@svgr/rollup": "^6.2.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/mocha": "^9.1.1",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "all-contributors-cli": "^6.20.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react-app": "^10.0.1",
    "cpy-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-watch": "^8.0.0",
    "gh-pages": "^3.2.3",
    "glob": "^8.0.1",
    "husky": "^8.0.0",
    "jasmine-expect": "^5.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-pnp-resolver": "^1.2.2",
    "jest-resolve": "^28.1.0",
    "jest-watch-typeahead": "^1.1.0",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-app-polyfill": "^3.0.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.1.0",
    "react-error-overlay": "^6.0.11",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.72.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "sort-package-json": "^1.57.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1"
  },
  "peerDependencies": {
    "plyr": "^3.7.2",
    "react": ">=16.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
